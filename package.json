{
  "name": "agro-gestion-app",
  "version": "1.0.0",
  "description": "Aplicación de gestión agrícola con manejo centralizado de stock",
  "author": "Tu Nombre",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/lab": "^5.0.0-alpha.150",
    "@mui/material": "^5.14.15",
    "@mui/x-data-grid": "^6.16.3",
    "@mui/x-date-pickers": "^6.16.3",
    "@supabase/supabase-js": "^2.49.4",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "prompt-sync": "^4.2.0",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "npm run react-build && electron-builder",
    "create-admin": "node scripts/createAdmin.js"
  },
  "build": {
    "appId": "com.tuempresa.agrogestion",
    "productName": "AgroGestión",
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public/assets"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/assets/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
